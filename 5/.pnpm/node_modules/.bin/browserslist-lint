#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ryanle/Desktop/projects/ryanmeetup/5/.pnpm/update-browserslist-db@1.0.10_browserslist@4.21.5/node_modules/update-browserslist-db/node_modules:/Users/ryanle/Desktop/projects/ryanmeetup/5/.pnpm/update-browserslist-db@1.0.10_browserslist@4.21.5/node_modules:/Users/ryanle/Desktop/projects/ryanmeetup/5/.pnpm/node_modules:/Users/ryanle/Desktop/projects/ryanmeetup/5/node_modules:/Users/ryanle/Desktop/projects/ryanmeetup/node_modules:/Users/ryanle/Desktop/projects/node_modules:/Users/ryanle/Desktop/node_modules:/Users/ryanle/node_modules:/Users/node_modules:/node_modules:5/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ryanle/Desktop/projects/ryanmeetup/5/.pnpm/update-browserslist-db@1.0.10_browserslist@4.21.5/node_modules/update-browserslist-db/node_modules:/Users/ryanle/Desktop/projects/ryanmeetup/5/.pnpm/update-browserslist-db@1.0.10_browserslist@4.21.5/node_modules:/Users/ryanle/Desktop/projects/ryanmeetup/5/.pnpm/node_modules:/Users/ryanle/Desktop/projects/ryanmeetup/5/node_modules:/Users/ryanle/Desktop/projects/ryanmeetup/node_modules:/Users/ryanle/Desktop/projects/node_modules:/Users/ryanle/Desktop/node_modules:/Users/ryanle/node_modules:/Users/node_modules:/node_modules:5/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../update-browserslist-db/cli.js" "$@"
else
  exec node  "$basedir/../update-browserslist-db/cli.js" "$@"
fi
