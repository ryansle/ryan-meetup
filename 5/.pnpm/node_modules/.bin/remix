#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ryanle/Desktop/projects/ryan-meetup/5/.pnpm/@vercel+remix-run-dev@1.16.0-patch.1/node_modules/@vercel/remix-run-dev/dist/node_modules:/Users/ryanle/Desktop/projects/ryan-meetup/5/.pnpm/@vercel+remix-run-dev@1.16.0-patch.1/node_modules/@vercel/remix-run-dev/node_modules:/Users/ryanle/Desktop/projects/ryan-meetup/5/.pnpm/@vercel+remix-run-dev@1.16.0-patch.1/node_modules/@vercel/node_modules:/Users/ryanle/Desktop/projects/ryan-meetup/5/.pnpm/@vercel+remix-run-dev@1.16.0-patch.1/node_modules:/Users/ryanle/Desktop/projects/ryan-meetup/5/.pnpm/node_modules:/Users/ryanle/Desktop/projects/ryan-meetup/5/node_modules:/Users/ryanle/Desktop/projects/ryan-meetup/node_modules:/Users/ryanle/Desktop/projects/node_modules:/Users/ryanle/Desktop/node_modules:/Users/ryanle/node_modules:/Users/node_modules:/node_modules:5/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ryanle/Desktop/projects/ryan-meetup/5/.pnpm/@vercel+remix-run-dev@1.16.0-patch.1/node_modules/@vercel/remix-run-dev/dist/node_modules:/Users/ryanle/Desktop/projects/ryan-meetup/5/.pnpm/@vercel+remix-run-dev@1.16.0-patch.1/node_modules/@vercel/remix-run-dev/node_modules:/Users/ryanle/Desktop/projects/ryan-meetup/5/.pnpm/@vercel+remix-run-dev@1.16.0-patch.1/node_modules/@vercel/node_modules:/Users/ryanle/Desktop/projects/ryan-meetup/5/.pnpm/@vercel+remix-run-dev@1.16.0-patch.1/node_modules:/Users/ryanle/Desktop/projects/ryan-meetup/5/.pnpm/node_modules:/Users/ryanle/Desktop/projects/ryan-meetup/5/node_modules:/Users/ryanle/Desktop/projects/ryan-meetup/node_modules:/Users/ryanle/Desktop/projects/node_modules:/Users/ryanle/Desktop/node_modules:/Users/ryanle/node_modules:/Users/node_modules:/node_modules:5/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@remix-run/dev/dist/cli.js" "$@"
else
  exec node  "$basedir/../@remix-run/dev/dist/cli.js" "$@"
fi
