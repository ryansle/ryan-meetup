#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ryanle/Desktop/projects/ryan-meetup/5/.pnpm/sort-package-json@1.57.0/node_modules/sort-package-json/node_modules:/Users/ryanle/Desktop/projects/ryan-meetup/5/.pnpm/sort-package-json@1.57.0/node_modules:/Users/ryanle/Desktop/projects/ryan-meetup/5/.pnpm/node_modules:/Users/ryanle/Desktop/projects/ryan-meetup/5/node_modules:/Users/ryanle/Desktop/projects/ryan-meetup/node_modules:/Users/ryanle/Desktop/projects/node_modules:/Users/ryanle/Desktop/node_modules:/Users/ryanle/node_modules:/Users/node_modules:/node_modules:5/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ryanle/Desktop/projects/ryan-meetup/5/.pnpm/sort-package-json@1.57.0/node_modules/sort-package-json/node_modules:/Users/ryanle/Desktop/projects/ryan-meetup/5/.pnpm/sort-package-json@1.57.0/node_modules:/Users/ryanle/Desktop/projects/ryan-meetup/5/.pnpm/node_modules:/Users/ryanle/Desktop/projects/ryan-meetup/5/node_modules:/Users/ryanle/Desktop/projects/ryan-meetup/node_modules:/Users/ryanle/Desktop/projects/node_modules:/Users/ryanle/Desktop/node_modules:/Users/ryanle/node_modules:/Users/node_modules:/node_modules:5/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sort-package-json/cli.js" "$@"
else
  exec node  "$basedir/../sort-package-json/cli.js" "$@"
fi
