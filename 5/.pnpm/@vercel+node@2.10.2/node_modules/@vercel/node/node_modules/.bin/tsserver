#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ryanle/Desktop/projects/ryanmeetup/5/.pnpm/typescript@4.3.4/node_modules/typescript/bin/node_modules:/Users/ryanle/Desktop/projects/ryanmeetup/5/.pnpm/typescript@4.3.4/node_modules/typescript/node_modules:/Users/ryanle/Desktop/projects/ryanmeetup/5/.pnpm/typescript@4.3.4/node_modules:/Users/ryanle/Desktop/projects/ryanmeetup/5/.pnpm/node_modules:/Users/ryanle/Desktop/projects/ryanmeetup/5/node_modules:/Users/ryanle/Desktop/projects/ryanmeetup/node_modules:/Users/ryanle/Desktop/projects/node_modules:/Users/ryanle/Desktop/node_modules:/Users/ryanle/node_modules:/Users/node_modules:/node_modules:5/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ryanle/Desktop/projects/ryanmeetup/5/.pnpm/typescript@4.3.4/node_modules/typescript/bin/node_modules:/Users/ryanle/Desktop/projects/ryanmeetup/5/.pnpm/typescript@4.3.4/node_modules/typescript/node_modules:/Users/ryanle/Desktop/projects/ryanmeetup/5/.pnpm/typescript@4.3.4/node_modules:/Users/ryanle/Desktop/projects/ryanmeetup/5/.pnpm/node_modules:/Users/ryanle/Desktop/projects/ryanmeetup/5/node_modules:/Users/ryanle/Desktop/projects/ryanmeetup/node_modules:/Users/ryanle/Desktop/projects/node_modules:/Users/ryanle/Desktop/node_modules:/Users/ryanle/node_modules:/Users/node_modules:/node_modules:5/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../typescript@4.3.4/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../../../typescript@4.3.4/node_modules/typescript/bin/tsserver" "$@"
fi
